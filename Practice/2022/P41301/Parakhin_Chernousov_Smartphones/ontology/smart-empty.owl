#################################################################
#    Classes
#################################################################

###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#Chip
smh:Chip rdf:type owl:Class ;
         rdfs:subClassOf [ rdf:type owl:Restriction ;
                           owl:onProperty smh:isChipOf ;
                           owl:someValuesFrom smh:Smartphone
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty smh:chipName ;
                           owl:someValuesFrom rdfs:Literal
                         ] ;
         rdfs:label "Чип"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#Compact
smh:Compact rdf:type owl:Class ;
            owl:equivalentClass [ owl:intersectionOf ( smh:Smartphone
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty smh:hasDisplay ;
                                                         owl:someValuesFrom [ rdf:type owl:Restriction ;
                                                                              owl:onProperty smh:diagonal ;
                                                                              owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                   owl:onDatatype xsd:float ;
                                                                                                   owl:withRestrictions ( [ xsd:maxExclusive "6.1"^^xsd:float
                                                                                                                          ]
                                                                                                                        )
                                                                                                 ]
                                                                            ]
                                                       ]
                                                       [ rdf:type owl:Restriction ;
                                                         owl:onProperty smh:weight ;
                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                              owl:onDatatype xsd:float ;
                                                                              owl:withRestrictions ( [ xsd:maxExclusive "160.0"^^xsd:float
                                                                                                     ]
                                                                                                   )
                                                                            ]
                                                       ]
                                                     ) ;
                                  rdf:type owl:Class
                                ] ;
            rdfs:label "Компактные смартфоны"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#Company
smh:Company rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty smh:hasCountry ;
                              owl:someValuesFrom smh:Country
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:companyName ;
                              owl:someValuesFrom rdfs:Literal
                            ] ;
            rdfs:label "Бренд"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#Country
smh:Country rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty smh:isCountryOf ;
                              owl:someValuesFrom smh:Company
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:countryName ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:label "Страна-производитель"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#Display
smh:Display rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty smh:density ;
                              owl:someValuesFrom xsd:integer
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:diagonal ;
                              owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onDataRange xsd:float
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:displayType ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:resolution ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty smh:resolutionType ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ;
            rdfs:label "Дисплей"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#ForBlogging
smh:ForBlogging rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( smh:Smartphone
                                                           [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty smh:hasDisplay ;
                                                                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                                                                owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                                                                owl:onProperty smh:resolutionType ;
                                                                                                                                                owl:hasValue "Full HD+"
                                                                                                                                              ]
                                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                                owl:onProperty smh:resolutionType ;
                                                                                                                                                owl:hasValue "WQHD"
                                                                                                                                              ]
                                                                                                                                            )
                                                                                                                              ]
                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty smh:frequency ;
                                                                                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                                     owl:onDatatype xsd:integer ;
                                                                                                                                                     owl:withRestrictions ( [ xsd:minInclusive 90
                                                                                                                                                                            ]
                                                                                                                                                                          )
                                                                                                                                                   ]
                                                                                                                              ]
                                                                                                                            ) ;
                                                                                                         rdf:type owl:Class
                                                                                                       ]
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty smh:hasDisplay ;
                                                                                    owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty smh:density ;
                                                                                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                                     owl:onDatatype xsd:integer ;
                                                                                                                                                     owl:withRestrictions ( [ xsd:minInclusive 360
                                                                                                                                                                            ]
                                                                                                                                                                          )
                                                                                                                                                   ]
                                                                                                                              ]
                                                                                                                              [ rdf:type owl:Restriction ;
                                                                                                                                owl:onProperty smh:diagonal ;
                                                                                                                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                                                     owl:onDatatype xsd:float ;
                                                                                                                                                     owl:withRestrictions ( [ xsd:minInclusive "6.1"^^xsd:float
                                                                                                                                                                            ]
                                                                                                                                                                          )
                                                                                                                                                   ]
                                                                                                                              ]
                                                                                                                            ) ;
                                                                                                         rdf:type owl:Class
                                                                                                       ]
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty smh:RAM ;
                                                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                         owl:onDatatype xsd:integer ;
                                                                                                         owl:withRestrictions ( [ xsd:minInclusive 4
                                                                                                                                ]
                                                                                                                              )
                                                                                                       ]
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty smh:ROM ;
                                                                                    owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                         owl:onDatatype xsd:integer ;
                                                                                                         owl:withRestrictions ( [ xsd:minInclusive 128
                                                                                                                                ]
                                                                                                                              )
                                                                                                       ]
                                                                                  ]
                                                                                  [ rdf:type owl:Restriction ;
                                                                                    owl:onProperty smh:fastCharge ;
                                                                                    owl:hasValue "true"^^xsd:boolean
                                                                                  ]
                                                                                ) ;
                                                             rdf:type owl:Class
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty smh:cameraScore ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:integer ;
                                                                                  owl:withRestrictions ( [ xsd:minExclusive 10
                                                                                                         ]
                                                                                                       )
                                                                                ]
                                                           ]
                                                         ) ;
                                      rdf:type owl:Class
                                    ] ;
                rdfs:label "Для блоггеров"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#ForBusiness
smh:ForBusiness rdf:type owl:Class ;
                owl:equivalentClass [ owl:intersectionOf ( smh:Smartphone
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty smh:hasDisplay ;
                                                             owl:someValuesFrom [ owl:intersectionOf ( [ rdf:type owl:Class ;
                                                                                                         owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                                                                         owl:onProperty smh:resolutionType ;
                                                                                                                         owl:hasValue "Full HD+"
                                                                                                                       ]
                                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                                         owl:onProperty smh:resolutionType ;
                                                                                                                         owl:hasValue "WQHD"
                                                                                                                       ]
                                                                                                                     )
                                                                                                       ]
                                                                                                       [ rdf:type owl:Restriction ;
                                                                                                         owl:onProperty smh:frequency ;
                                                                                                         owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                                                              owl:onDatatype xsd:integer ;
                                                                                                                              owl:withRestrictions ( [ xsd:minInclusive 90
                                                                                                                                                     ]
                                                                                                                                                   )
                                                                                                                            ]
                                                                                                       ]
                                                                                                     ) ;
                                                                                  rdf:type owl:Class
                                                                                ]
                                                           ]
                                                           [ rdf:type owl:Restriction ;
                                                             owl:onProperty smh:RAM ;
                                                             owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                                                  owl:onDatatype xsd:integer ;
                                                                                  owl:withRestrictions ( [ xsd:minInclusive 4
                                                                                                         ]
                                                                                                       )
