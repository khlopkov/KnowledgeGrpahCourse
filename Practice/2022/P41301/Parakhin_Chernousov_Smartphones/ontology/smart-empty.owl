#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#hasChip
smh:hasChip rdf:type owl:ObjectProperty ;
            owl:inverseOf smh:isChipOf ;
            rdf:type owl:FunctionalProperty ,
                     owl:AsymmetricProperty ;
            rdfs:domain smh:Smartphone ;
            rdfs:range smh:Chip ;
            rdfs:label "имеет чип"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#hasCompany
smh:hasCompany rdf:type owl:ObjectProperty ;
               owl:inverseOf smh:isCompanyOf ;
               rdf:type owl:AsymmetricProperty ;
               rdfs:domain smh:Smartphone ;
               rdfs:range smh:Company ;
               rdfs:label "произведен"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#hasCountry
smh:hasCountry rdf:type owl:ObjectProperty ;
               owl:inverseOf smh:isCountryOf ;
               rdf:type owl:AsymmetricProperty ;
               rdfs:domain smh:Company ;
               rdfs:range smh:Country ;
               rdfs:label "принадлежит стране"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#hasDisplay
smh:hasDisplay rdf:type owl:ObjectProperty ;
               owl:inverseOf smh:isDisplayOf ;
               rdf:type owl:FunctionalProperty ,
                        owl:AsymmetricProperty ;
               rdfs:domain smh:Smartphone ;
               rdfs:range smh:Display ;
               rdfs:label "имеет дисплей"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#isChipOf
smh:isChipOf rdf:type owl:ObjectProperty ,
                      owl:AsymmetricProperty ;
             rdfs:domain smh:Chip ;
             rdfs:range smh:Smartphone ;
             rdfs:label "чип установлен на"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#isCompanyOf
smh:isCompanyOf rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain smh:Company ;
                rdfs:range smh:Smartphone ;
                rdfs:label "бренд производит модель"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#isCountryOf
smh:isCountryOf rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain smh:Country ;
                rdfs:range smh:Company ;
                rdfs:label "страна производит смартфоны компании"@ru .


###  http://www.semanticweb.org/eugene/ontologies/2022/1/smartphone-1#isDisplayOf
smh:isDisplayOf rdf:type owl:ObjectProperty ,
                         owl:AsymmetricProperty ;
                rdfs:domain smh:Display ;
                rdfs:range smh:Smartphone ;
                rdfs:label "дисплей установлен на"@ru .


